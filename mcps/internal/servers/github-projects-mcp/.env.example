# Token d'accès personnel GitHub avec les permissions nécessaires pour GitHub Projects
GITHUB_TOKEN=your_github_token_here

# Configuration du serveur MCP
MCP_PORT=3000
MCP_HOST=localhost

# Configuration de synchronisation (optionnel)
SYNC_INTERVAL=300000  # Intervalle de synchronisation en millisecondes (5 minutes par défaut)

# Configuration de journalisation
LOG_LEVEL=info  # debug, info, warn, error