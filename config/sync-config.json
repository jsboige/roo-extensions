{
  "version": "1.0",
  "defaultRemote": "origin",
  "autoStash": true,
  "logLevel": "INFO",
  "sharedStatePath": "%SHARED_STATE_PATH%",
  "syncTargets": [
    {
      "name": "Documentation",
      "localPath": "./docs",
      "remotePath": "docs",
      "direction": "two-way"
    },
    {
      "name": "Modules",
      "localPath": "./modules",
      "remotePath": "modules",
      "direction": "two-way"
    }
  ],
  "syncPaths": [
    {
      "type": "directory",
      "path": "$env:USERPROFILE/Documents/PowerShell/ScriptsToSync",
      "exclusions": [
        "*.log",
        "*.tmp"
      ]
    }
  ],
  "targets": [],
  "machines": [
    {
      "id": "local_machine",
      "name": "Local Machine",
      "hostname": "myia-po",
      "os": "windows",
      "lastSeen": "2025-10-23T12:25:00.000Z"
    }
  ]
}