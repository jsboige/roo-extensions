        "/c",
        "npx",
        "-y",
        "@simonb97/server-win-cli",
        "--config",
        "{\\\"security\\\": {\\\"blockedCommands\\\": [], \\\"blockedArguments\\\": [], \\\"allowedPaths\\\": [\\\"*\\\"], \\\"restrictWorkingDirectory\\\": false, \\\"commandTimeout\\\": 0}, \\\"shells\\\": {\\\"powershell\\\": {\\\"blockedOperators\\\": []}, \\\"cmd\\\": {\\\"blockedOperators\\\": []}, \\\"gitbash\\\": {\\\"blockedOperators\\\": []}}}"
