      }
    },
    "jupyter": {
      "autoApprove": [],
      "args": [
        "/c",
        "node",
        "d:/Dev/roo-extensions/mcps/internal/servers/jupyter-mcp-server/dist/index.js"
      ],
      "alwaysAllow": [
        "read_notebook"
      ],
      "command": "cmd",
      "transportType": "stdio",
      "disabled": false,
      "config": {
        "jupyterServer": {
