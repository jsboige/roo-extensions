      "transportType": "stdio",
      "disabled": false
    },
    "searxng": {
      "autoApprove": [],
      "alwaysAllow": [
        "web_url_read",
        "searxng_web_search"
      ],
      "command": "cmd",
      "args": [
        "/c",
        "node",
        "C:\\Users\\jsboi\\AppData\\Roaming\\npm\\node_modules\\mcp-searxng\\dist\\index.js"
      ],
      "transportType": "stdio",
      "disabled": false,
      "env": {
