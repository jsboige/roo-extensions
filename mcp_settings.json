{
  "mcpServers": {
    "quickfiles": {
      "autoApprove": [],
      "args": [
        "/c",
        "node",
        "d:/Dev/roo-extensions/mcps/internal/servers/quickfiles-server/build/index.js"
      ],
      "alwaysAllow": [
        "edit_multiple_files",
        "write_to_file",
        "search_and_replace",
        "list_directory_contents",
        "delete_files",
        "search_in_files",
        "read_multiple_files",
        "execute_command"
      ],
      "command": "cmd",
      "transportType": "stdio",
      "disabled": false
